{
  "name": "react-minimist-boilerplate",
  "version": "0.0.2",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "npm run lint && vite --config ./config/vite.config.ts",
    "build": "tsc && vite build --config ./config/vite.config.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "lint:max": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 5",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate": "plop --plopfile ./plop/plopfile.js --debug",
    "test": "npx playwright test --config ./config/playwright.config.ts",
    "test-ui": "npx playwright test --ui --config ./config/playwright.config.ts"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "dotenv": "^16.3.1",
    "framer-motion": "^10.16.16",
    "plop": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-minimist-utils": "^0.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@playwright/test": "^1.40.1",
    "@storybook/addon-essentials": "^7.0.0-rc.5",
    "@storybook/addon-interactions": "^7.0.0-rc.5",
    "@storybook/addon-links": "^7.0.0-rc.5",
    "@storybook/blocks": "^7.0.0-rc.5",
    "@storybook/react": "^7.0.0-rc.5",
    "@storybook/react-vite": "^7.0.0-rc.5",
    "@storybook/test": "^7.6.0-alpha.3",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@svgr/rollup": "^8.1.0",
    "@types/dompurify": "^3.0.4",
    "@types/node": "^20.10.4",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "concurrently": "^7.6.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.6.15",
    "postcss": "^8.4.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.69.5",
    "storybook": "^7.0.0-rc.5",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^1.7.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-tsconfig-paths": "^4.0.3"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
